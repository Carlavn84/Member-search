{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [ "prettier"],
    "plugins": [ "vue"],
    "parser" : "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "ecmaVersion": 2020
    },
    "rules": {
        "prettier/prettier": ["error"],
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "semi": 1,
        "vue/html-indent": "warn"
        
     

    }
  }

  
